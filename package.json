{
  "name": "funcy",
  "version": "0.0.1",
  "type": "module",
  "private": true,
  "scripts": {
    "preinstall": "npx -y only-allow pnpm",
    "test": "vitest run --coverage",
    "test:watch": "vitest --ui",
    "typecheck": "pnpm run -r typecheck",
    "lint": "eslint --cache ./packages/",
    "format": "prettier --ignore-path .gitignore --write --cache ."
  },
  "devDependencies": {
    "@tsconfig/node20": "^20.1.4",
    "@types/airbnb__node-memwatch": "^2.0.3",
    "@types/aws-lambda": "^8.10.137",
    "@types/node": "^20.12.7",
    "@vitest/coverage-istanbul": "^1.5.0",
    "@vitest/ui": "^1.5.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.2.5",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.7.0",
    "vitest": "^1.5.0"
  },
  "main": "index.ts",
  "author": "refactorthis",
  "license": "ISC",
  "description": "**funcy** provides a performant, strongly typed, and functional interface for AWS lambda function handlers.",
  "workspaces": [
    "packages/*"
  ]
